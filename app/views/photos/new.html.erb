<div class="container row">
    <%= render partial: "home/option_feeds_discover" %>
    <div class="col-sm-8 new-photo">
        <div class="title-new-photo">
            <h3><%= t('.new-photo') %></h3>
        </div>
        <div class="main-new-photo">
                <%= form_for([@user,@photo], :html => { :multipart => true }) do |f| %>
                    <%= render partial: "form_photo", locals: { type: "new", f: f, user: @user, photo: @photo } %>
                <% end %>
        </div>
    </div>    
</div>