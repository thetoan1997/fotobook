<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Products</h3>
  </div>
  <div class="table-responsive">
    <table class='table table-hover table-condensed'>
      <thead>
        <tr>
          <th>Name</th>
          <th>Price</th>
          <th>Department</th>
        </tr>
      </thead>

      <tbody id='photos'>
        <%= render @photos %>
      </tbody>
    </table>
  </div>
</div>
<%= will_paginate @photos %>
