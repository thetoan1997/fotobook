<div class="row">
    <% @photos_discover.each do |photo| %>
        <%= render partial: "photos_post", locals: { photo: photo, type: "discover" } %>
        <% unless photo.image.nil? %>
            <%= render partial: "modal_photo", 
                    locals: { url: (photo.image.image_link.attached? ? url_for(photo.image.image_link) : photo.image.image_url), 
                            title: photo.title, 
                            description: photo.description, 
                            id: "discover"+photo.id.to_s } %>
        <% end %>
    <% end %>
</div>
<%= paginate @photos_discover, param_name: "page_photo_discover" %>