<table class="table table-striped">
  <tr>
    <th><%= t('.firstname') %></th>
    <th><%= t('.lastname') %></th> 
    <th><%= t('.email') %></th>
    <th><%= t('.last-login') %></th>
    <th> </th>
  </tr>
  <% users.each do |user| %>
    <tr>
        <td><%= user.firstname %></td>
        <td><%= user.lastname %></td> 
        <td><%= user.email %></td>
        <td><%= user.current_sign_in_at %></td>
        <td>
          <%= link_to t('.edit'), edit_user_path(id: user.id), class: "btn btn-success show-edit-photo" %>
          <%= link_to t('.delete'), user_path(user), method: :delete, data: { confirm: 'Are you sure?'} , class: "btn btn-danger show-edit-photo " %>
        </td>
    </tr>
  <% end %>
</table>