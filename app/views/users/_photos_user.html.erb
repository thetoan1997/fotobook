<% images.each do |image| %>
<div class="col-lg-3 col-md-6">
    <div class="pictures-in-user">
        <% if image.image_link.attached? %>
            <img class="picture-post" src=<%= url_for(image.image_link) %>>
        <% end %>
    </div>
    <p class="title-in-picture-user">image.imageable.title</p>
</div>
<% end %>
